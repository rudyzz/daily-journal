<%- include("partials/header") %>

    <h1>Home</h1>
    <p><%= homeStartingContent %> </p>

    <% posts.forEach(element => { %>
        <h2> <%= element.title %> </h2>
        <p>
            <%= element.body.substring(0,100) + "..." %>
            <a href="/posts/<%=  element._id %>"> Read More </a>
        </p> 
    <% }) %>

<%- include("partials/footer") %>
